    const loginBtn = document.getElementById("loginBtn");
    const modal = document.getElementById("modal");

    loginBtn.addEventListener("click", function() {
      modal.style.display = "flex";
    });

    function closeModal() {
      modal.style.display = "none";
    }
  </script>

