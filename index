<script>
  function toggleFaq(element) {
    // Pobranie odpowiedzi (faq-answer)
    const answer = element.nextElementSibling;
    
    // Prze≈ÇƒÖczanie widoczno≈õci odpowiedzi
    if (answer.style.display === "none" || answer.style.display === "") {
      answer.style.display = "block";  // Poka≈º odpowied≈∫
      element.querySelector('.arrow').textContent = "‚ñ≤";  // Zmie≈Ñ strza≈Çkƒô na g√≥rƒô
    } else {
      answer.style.display = "none";  // Ukryj odpowied≈∫
      element.querySelector('.arrow').textContent = "‚ñº";  // Zmie≈Ñ strza≈Çkƒô na d√≥≈Ç
    }
  }
</script>

</body>
